{
    "manifest_version": 2,
  
    "name": "Click to execute",
    "description": "Click to execute an alert function",
    "version": "1.0",
  
    "icons": {
      "48": "icon.png"
    },
  
    "permissions": [
      "tabs", "<all_urls>"
    ],
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
  
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }